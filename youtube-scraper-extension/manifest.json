{
  "manifest_version": 3,
  "name": "YouTube Channel Scraper",
  "version": "1.2.0",
  "description": "抓取当前 YouTube 频道页面的所有视频信息 (标题, 链接, 浏览量, 发布时间, 是否会员).",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}